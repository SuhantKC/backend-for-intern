generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "sqlite"
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String
  price       Float
  category    String
  stock       Int
  description String
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  username String @unique
  password String
}
